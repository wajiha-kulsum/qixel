{
  "name": "@wajiha12/qixel-cli",
  "version": "0.1.2",
  "description": "CLI tool for adding animated React components to your project",
  "type": "module",
  "bin": {
    "qixel": "dist/index.js"
  },
  "files": [
    "dist",
    "components"
  ],
  "keywords": [
    "cli",
    "react",
    "components",
    "animation",
    "framer-motion",
    "ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wajiha-kulsum/qixel.git",
    "directory": "packages/cli"
  },
  "bugs": {
    "url": "https://github.com/wajiha-kulsum/qixel/issues"
  },
  "homepage": "https://github.com/wajiha-kulsum/qixel#readme",
  "license": "MIT",
  "scripts": {
    "build": "tsup src/index.ts --format esm --dts && npm run copy-registry",
    "copy-registry": "node -e \"require('fs').cpSync('../components/registry.json', 'components/registry.json', {recursive: true, force: true}); require('fs').mkdirSync('components', {recursive: true})\"",
    "dev": "tsup src/index.ts --format esm --watch",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "framer-motion": "^12.23.24",
    "ora": "^8.1.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/prompts": "^2.4.4",
    "tsup": "^8.0.0",
    "typescript": "^5.6.0"
  }
}
